networks:
  my-network:
    external: true
    name: my-network
services:
  erthsobes-db:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobes-db/Dockerfile
    environment:
      POSTGRES_DB: orders
      POSTGRES_PASSWORD: orders
      POSTGRES_USER: orders
    image: erthsobes/erthsobes-db
    networks:
      my-network:
        ipv4_address: 172.23.0.5
    ports:
    - published: 5432
      target: 5432
  erthsobesapi:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobesapi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:6570
      DOTNET_RUNNING_IN_CONTAINER: "true"
    image: erthsobes/erthsobesapi
    networks:
      my-network:
        ipv4_address: 172.23.0.2
    ports:
    - published: 6570
      target: 6570
  erthsobesservice:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobesservice/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:6500
      DOTNET_RUNNING_IN_CONTAINER: "true"
    image: erthsobes/erthsobesservis
    networks:
      my-network:
        ipv4_address: 172.23.0.4
    ports:
    - published: 6500
      target: 6500
version: '3.8'