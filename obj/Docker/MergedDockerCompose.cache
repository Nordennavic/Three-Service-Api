services:
  erthsobes-api:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobes-api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:6570
      DOTNET_RUNNING_IN_CONTAINER: "true"
    image: erthsobes/erthsobes-api
    ports:
    - published: 6570
      target: 6570
  erthsobes-db:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobes-db/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:5432
      POSTGRES_DB: orders
      POSTGRES_PASSWORD: orders
      POSTGRES_USER: orders
    image: erthsobes/erthsobes-db
    ports:
    - published: 5432
      target: 5432
    volumes:
    - C:\Users\Denis\Documents\GitHub\Three-Services-Api\.db:/var/lib/postgresql/data:rw
  erthsobesservice:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobesservice/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:6500
      DOTNET_RUNNING_IN_CONTAINER: "true"
    image: erthsobes/erthsobesservis
    ports:
    - published: 6500
      target: 6500
version: '3.8'