networks:
  my-network:
    external: true
    name: my-network
services:
  erthsobes-api:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
      dockerfile: erthsobes-api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: erthsobes-api
      target: base
    container_name: erthsobes-api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:6570
      DOTNET_RUNNING_IN_CONTAINER: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: erthsobes/erthsobes-api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/erthsobes-api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      my-network: {}
    ports:
    - published: 6570
      target: 80
    tty: true
    volumes:
    - C:\Users\Denis\Documents\GitHub\Three-Services-Api\erthsobes-api:/app:rw
    - C:\Users\Denis\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Denis\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Denis\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Denis\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Denis\Documents\GitHub\Three-Services-Api:/src:rw
  erthsobes-db:
    build:
      context: C:\Users\Denis\Documents\GitHub\Three-Services-Api
